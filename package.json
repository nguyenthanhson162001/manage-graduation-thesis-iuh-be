{
	"name": "manage-graduation-thesis-iuh",
	"version": "1.0.0",
	"description": "manage graduation thesis iuh",
	"author": "nguyenthanhson162001@gmail.com",
	"license": "ISC",
	"scripts": {
		"build": "node ./tools/scripts/build.js",
		"start": "node -r ts-node/register -r tsconfig-paths/register ./dist",
		"local": "nodemon",
		"develop": "SET NODE_ENV=develop&&nodemon",
		"deploy": "npm run start",
		"lint:fix": "eslint ./src/**/*.ts --fix",
		"lint:staged": "lint-staged",
		"migrate:make": "knex --knexfile src/@core/infrastructure/knex/index.ts migrate:make",
		"migrate:latest": "knex --knexfile src/@core/infrastructure/knex/index.ts migrate:latest",
		"migrate:rollback": "knex --knexfile src/@core/infrastructure/knex/index.ts migrate:rollback",
		"core:repository:create": "node ./tools/scripts/create-mongoose-core-repository/index.js --module @core --repository ",
		"api:repository:create": "node ./tools/scripts/create-mongoose-modules-extends-repository/index.js --module api --repository "
	},
	"repository": {
		"type": "git",
		"url": "https://"
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": "ts, html",
		"ignore": [
			"src/public"
		],
		"exec": "node --inspect -r ts-node/register -r tsconfig-paths/register ./src"
	},
	"lint-staged": {
		"./src/**/*.ts": [
			"prettier --write --config ./.prettierrc --ignore-path ./.prettierignore",
			"eslint"
		]
	},
	"_moduleAliases": {
		"@core/*": [
			"dist/@core"
		],
		"@admin": [
			"dist/admin"
		],
		"@lecturer": [
			"dist/lecturer"
		],
		"@student": [
			"dist/student"
		],
		"@env": [
			"env"
		]
	},
	"dependencies": {
		"@types/ejs": "^3.0.4",
		"@types/express-flash": "0.0.2",
		"@types/express-session": "^1.17.0",
		"@types/http-errors": "^2.0.1",
		"@types/lodash": "^4.14.161",
		"@types/nodemailer": "^6.4.0",
		"axios": "^0.22.0",
		"bcrypt": "^5.1.0",
		"blob-stream": "^0.1.3",
		"cloudinary": "^1.34.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"csv-parser": "^3.0.0",
		"dotenv": "^8.2.0",
		"ejs": "^3.1.5",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"express-flash": "0.0.2",
		"express-session": "^1.17.0",
		"fs-extra": "^9.0.1",
		"glob": "^7.1.7",
		"handlebars": "^4.7.7",
		"helmet": "^4.6.0",
		"http-error": "^0.0.6",
		"http-errors": "^2.0.0",
		"inversify": "^5.1.1",
		"joi": "^17.7.0",
		"jsonwebtoken": "^8.5.1",
		"knex": "^2.4.2",
		"lodash": "^4.17.21",
		"module-alias": "^2.2.2",
		"mongoose": "^5.10.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.4",
		"multer-storage-cloudinary": "^4.0.0",
		"mysql": "^2.18.1",
		"nodemailer": "^6.9.1",
		"objection": "^3.0.1",
		"pdfkit": "^0.13.0",
		"puppeteer": "^19.10.1",
		"redis": "^4.6.4",
		"reflect-metadata": "^0.1.13",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^4.6.1",
		"ts-node": "^10.9.1",
		"urlencode": "^1.1.0",
		"uuid": "^9.0.0",
		"winston": "^3.3.3",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@types/bcrypt": "^5.0.0",
		"@types/blob-stream": "^0.1.30",
		"@types/command-line-args": "^5.0.0",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.7",
		"@types/express": "^4.17.8",
		"@types/find": "^0.2.1",
