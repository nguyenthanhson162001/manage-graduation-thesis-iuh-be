		"@types/multer": "^1.4.5",
		"@types/node": "^14.6.3",
		"@types/pdfkit": "^0.12.9",
		"@types/supertest": "^2.0.10",
		"@types/swagger-jsdoc": "^6.0.1",
		"@types/swagger-ui-express": "^4.1.3",
		"@types/urlencode": "^1.1.2",
		"@types/uuid": "^9.0.0",
		"jasmine": "^3.6.1",
		"nodemon": "^2.0.4",
		"supertest": "^4.0.2",
		"tsconfig-paths": "^3.9.0",
		"tslint": "^6.1.3",
		"typescript": "^4.7.4"
	},
	"engines": {
		"npm": "8.19.3",
		"node": "18.13.0"
	}
}
